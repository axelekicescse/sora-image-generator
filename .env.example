# Sora Image Generator Configuration

# URL for the Sora website
SORA_URL=https://sora.com

# Browser settings
HEADLESS=False

# Logging configuration
LOG_LEVEL=INFO
LOG_DIR=logs

# Retry mechanism settings
MAX_RETRIES=3
RETRY_DELAY=2

# Timeout settings (in seconds)
WAIT_TIMEOUT=60
GENERATION_TIMEOUT=600  # 10 minutes for long image generation
MAX_SESSION_TIME=600

# Output directories
IMAGE_DIR=images
